{% extends 'base.html.twig' %}

{% block title %}Vaccination{% endblock %}

{% block body %}
    <h1>Vaccination</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vaccination.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ vaccination.name }}</td>
            </tr>
            <tr>
                <th>Frequence</th>
                <td>{{ vaccination.frequence }}</td>
            </tr>
            <tr>
                <th>Dosage</th>
                <td>{{ vaccination.dosage }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ vaccination.date }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ vaccination.duration }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_vaccination_index') }}">back to list</a>

    <a href="{{ path('app_vaccination_edit', {'id': vaccination.id}) }}">edit</a>

    {{ include('vaccination/_delete_form.html.twig') }}
{% endblock %}
